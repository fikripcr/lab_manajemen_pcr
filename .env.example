APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost
# Turn on Laravel-debugbar
DEBUGBAR_ENABLED=true
# Turn off debugbar storage to prevent "file too large or create json on storage" error
DEBUGBAR_STORAGE_ENABLED=false

APP_LOCALE=id
APP_FALLBACK_LOCALE=id
APP_FAKER_LOCALE=id_ID

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

# PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=sqlite
# DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=laravel
# DB_USERNAME=root
# DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
DB_CACHE_TABLE=sys_cache
DB_CACHE_LOCK_TABLE=sys_cache_locks
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

# Database Backup Configuration
MYSQLDUMP_PATH="C:/laragon/bin/mysql/mysql-8.0.30-winx64/bin/mysqldump.exe"

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

# ==============================================================================
# Tabler UI Theme & Layout Configuration
# ==============================================================================
# These settings control the default appearance of the sys section.
# After changing these values, run: php artisan config:cache

# Theme: light, dark
TABLER_THEME=light

# Primary Color: blue, azure, indigo, purple, pink, red, orange, yellow, lime, green, teal, cyan
TABLER_THEME_PRIMARY=blue

# Font: sans-serif, serif, monospace, comic
TABLER_THEME_FONT=sans-serif

# Gray Base: slate, gray, zinc, neutral, stone
TABLER_THEME_BASE=gray

# Border Radius: 0, 0.5, 1, 1.5, 2
TABLER_THEME_RADIUS=1

# Layout (choose one): vertical, horizontal, combined, boxed, fluid, navbar-sticky, navbar-dark, vertical-dark
TABLER_LAYOUT=vertical


